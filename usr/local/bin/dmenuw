#!/bin/sh

source ~/.config/global.conf

PROMPT=">>"
DIM=0.35
X=540
Y=250
W=300
H=20
L=10

dmenu_run \
    -i -r -l $L \
    -p "$PROMPT" \
    -x $X -y $Y \
    -w $W -h $H \
    -fn $FONT2 \
    -dim $DIM \
    -nb "#$BAR_BACKGROUND" \
    -nf "#$BAR_FOREGROUND" \
    -sb "#$MAIN_COLOR" \
    -sf "#$BAR_BACKGROUND" \
